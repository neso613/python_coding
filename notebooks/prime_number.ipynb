    "from math import sqrt\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "876e8f85",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "def isPrime(n):\n",
    "    for i in range(2, int(sqrt(n) + 1)):\n",
    "        if n % i is 0:\n",
    "            return False\n",
    "    return True\n"
   ]
  },
