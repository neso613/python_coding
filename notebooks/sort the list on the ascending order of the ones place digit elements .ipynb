    "def unit_sort(ele):\n",
    "    return str(ele)[-1]\n",
    "test_list = [23,54,91,108,102]\n",
    "print(\"The original list is : \" + str(test_list))\n",
    "test_list.sort(key=unit_sort)\n",
    "print(\"The unit sorted list : \" + str(test_list))\n"
   ]
