    "def runner_up(arr):\n",
    "    lis = list(arr)\n",
    "    winner = max(lis)\n",
    "    s = set(lis)\n",
    "    s.remove(winner)\n",
    "    return max(s)\n",
    "arr = map(int, '2 3 6 6 5'.split())\n",
    "runner_up(arr)"
